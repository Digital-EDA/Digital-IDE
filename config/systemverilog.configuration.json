{
	"comments": {
		"lineComment": "//",
		"blockComment": [ "/*", "*/" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
        ["begin", "end"],
		["case", "endcase"],
		["class", "endclass"],
		["clocking", "endclocking"],
		["function", "endfunction"],
		["group", "endgroup"],
		["interface", "endinterface"],
		["module", "endmodule"],
		["package", "endpackage"],
		["primitive", "endprimitive"],
		["program", "endprogram"],
		["property", "endproperty"],
		["sequence", "endsequence"],
		["task", "endtask"]
	],
	"autoClosingPairs": [
		{"open":"(", "close":")", "notIn":["string", "comment"]},
		{"open":"[", "close":"]", "notIn":["string", "comment"]},
		{"open":"{", "close":"}", "notIn":["string", "comment"]},
		{"open":"\"", "close":"\"", "notIn":["string", "comment"]}
	]
}