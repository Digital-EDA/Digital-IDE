		"keybindings": [
			{
				"command": "digital-ide.tool.instance",
				"key": "alt+i",
				"mac": "alt+i",
				"when": "editorTextFocus"
			},
			{
				"command": "digital-ide.tool.testbench",
				"key": "alt+t",
				"mac": "alt+t",
				"when": "editorTextFocus"
			}
		],
		"languages": [
			{
				"id": "tcl",
				"aliases": [
					"TCL",
					"Xilinx Constraints File",
